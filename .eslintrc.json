{
    "env": {
        "es6": true,
        "browser": true
    },
    "extends": ["eslint:recommended"],
    "parserOptions": {
        "ecmaVersion": 2017,
        "sourceType": "module"
    },
    "rules": {
        "no-console": "off",
        "semi": 2,
        "no-unused-vars": "warn"
    }
}
